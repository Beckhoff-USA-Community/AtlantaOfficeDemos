<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="F_SetAllMoversMovement" Id="{b1e646bf-97ca-48c6-8736-aa752c85bfcf}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION F_SetAllMoversMovement : BOOL
VAR_INPUT
	Set			:ST_MoveDynamics;
END_VAR
VAR
	i			:UINT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[FOR i:=1 TO XPlanarApplication.SystemParameters.MOVER_COUNT DO
	IF ip.Movers[i]<>0 THEN
		ip.Movers[i].Dynamics.Movement:=Set;		
	END_IF			
END_FOR	]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>